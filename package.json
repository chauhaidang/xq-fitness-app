{
  "name": "xq-fitness-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:integration:watch": "jest --config=jest.integration.config.js --watch",
    "test:all": "npm run test:unit && npm run test:integration",
    "mock:api": "prism mock api/read-service-api.yaml api/write-service-api.yaml --errors",
    "bip": "expo prebuild --no-install --platform ios && cp build-sim.sh ios/build-sim.sh && chmod +x ios/build-sim.sh && cp build-device.sh ios/build-device.sh && chmod +x ios/build-device.sh",
    "bi": "cd ios && pod update && pod install && sh ./build-sim.sh && cd ..",
    "deploy-sim": "./deploy-sim.sh",
    "build-and-deploy": "npm run bi && npm run deploy-sim",
    "ls-device": "xcrun simctl list devices available",
    "bid": "GATEWAY_URL='https://xq-fitness-udfuy.ondigitalocean.app' cd ios && pod update && pod install && sh ./build-device.sh"
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.6.2",
    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "expo-dev-client": "~2.4.13",
    "expo-splash-screen": "~0.20.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@stoplight/prism-cli": "^5.4.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "cors": "^2.8.5",
    "detox": "^20.14.0",
    "express": "^4.18.2",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "jest-expo": "~49.0.0",
    "react-test-renderer": "^18.2.0",
    "expo-build-properties": "0.8.3"
  },
  "private": true
}
